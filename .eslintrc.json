{
  "root": true,
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "es2022": true
  },
  "extends": [
    "mdcs"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      "tab",
      {
        "ignoredNodes": [
          "TemplateLiteral"
        ]
      }
    ]
  }
}